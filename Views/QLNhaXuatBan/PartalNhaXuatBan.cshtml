@using BookShop_Online.Models
@model List<NhaXuatBan>

<div class="card" style="margin-bottom: 20px">
    <div class="card-heard" style="text-align: center">NHÀ XUẤT BẢN</div>
    <div class="card-body">
        <div style="border-radius: 4px; border: 1px solid gray">
            @foreach (var item in Model)
             {
                <div>
                    <ul class="list-group" style="border-radius: 6px; list-style-type: none">
                        <li style="border: 1px solid gray; padding-left: 20px">@item.TenNXB.</li>
                    </ul>
                </div>
             }
        </div>
    </div>
</div>
